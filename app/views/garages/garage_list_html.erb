<div>
  <p class="h3">
    <% if params[:query].blank? %>
      <%= pluralize garages.count, "garage" %> available
    <% else %>
      <%= pluralize garages.count, "result" %> for <code><%= params[:query] %></code>
    <% end %>
  </p>
  <div class="garages">
    <% @garages.each do |garage| %>
      <%= render "garage_info", garage: garage %>
    <% end %>
  </div>
</div>
